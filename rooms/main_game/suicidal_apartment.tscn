script/source = "extends SceneSwitcher

func interact_action():
#	print(\"olá!\")
	if Game.current_scene.can_exit_apartment:
		.interact_action()
		Audio.play_audio(load(\"res://assets/audio/sound_effects/door_opening_closing/door_opening.wav\"))
#		pass
	else:
		if Game.current_scene.case_intro_ended:
			var window = Dialogue.start_dialogue(\"main_game/suicidal_apartment/cant_exit\")
			window.get_node(\"base_dialogue_branch\").connect(\"dialogue_finished\", self, \"finished_talking\")
			Game.lock_player_input()
			Game.scene_manager.gui.lock_menu()
	

func finished_talking():
	terminate_interaction()
	Game.unlock_player_input()
	Game.scene_manager.gui.unlock_menu()
