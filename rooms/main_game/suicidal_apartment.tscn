script/source = "extends GameScene

var clue_counter = 0

var can_exit_apartment = false
var case_intro_ended = false

func _ready():
	._ready()
	yield(get_tree(),\"idle_frame\")
	Cutscene.start_cutscene(get_node(\"case_intro\"))
	Cutscene.connect(\"cutscene_ended\", self, \"case_intro_end\")


# Mostrar a ficha da vítima depois de terem conversado sobre a vítima

func case_intro_end():
	var criminal_report = preload(\"res://entities/base/components/scene/criminal_report_showcase/base_criminal_report_showcase.tscn\").instance()
	var samuel_report = load(\"res://assets/sprites/criminal_report_envelope/samuel_report.png\")
#	print(samuel_report)
	Game.scene_manager.gui.add_child(criminal_report)
	criminal_report.setup_and_play(samuel_report)
	criminal_report.connect(\"concluded_reading\", self, \"concluded_reading\")
	print(\"introdução do caso terminado! Mostrando ficha para o jogador\")

func concluded_reading():
	Game.unlock_player_input()
	Game.scene_manager.gui.unlock_menu()
	Game.scene_manager.set_current_camera(get_node(\"cameras/rooms/main_room_view\"))
	Inventory.give_item(load(\"res://database/items/evidences/samuel_report.tres\"))
	case_intro_ended  = true
	Audio.play_music(music)

# =====================================================================

# Entrar para discussão sobre o real destino da vítima
func finish_investigation():
#	print(\"Terminou?\")
	if clue_counter == 4:
		print(\"sim!\")
		Game.lock_player_input()
		Game.scene_manager.gui.lock_menu()
		var window = Dialogue.start_dialogue(\"main_game/suicidal_apartment/all_evidences_found\")
		window.get_node(\"base_dialogue_branch\").connect(\"dialogue_finished\", self, \"finished_evidence_dialogue\")
#		trandição pra preto e transportar o jogador pra perto do Leonnard
#		Conversa entre Gaspard e Leonnard começa


func finished_evidence_dialogue():
	var transition = Game.scene_manager.transition_overlay_node.instance()
	Game.scene_manager.gui.add_child(transition)
	transition.fade_in()
	
	yield(transition.animation, \"animation_finished\")
	
	Game.current_player.global_translation = Vector3(13.592, 0.286, -12.23)
	
#	Game.current_player.global_rotation.y = 48.636
	Game.current_player.look_at(get_node(\"modeloleonardd\").global_translation, Vector3.UP)
	get_node(\"modeloleonardd\").look_at(Game.current_player.global_translation, Vector3.UP)
	
	Game.scene_manager.set_current_camera(get_node(\"cameras/discussing_case_cutscene\"))
	
	transition.fade_out()
	
	yield(transition.animation, \"animation_finished\")
	
	var window = Dialogue.start_dialogue(\"main_game/suicidal_apartment/discussing_about_the_case\")
	window.get_node(\"base_dialogue_branch\").connect(\"dialogue_finished\", self, \"finished_discussion\")
#	Começa diálogo entre Gaspard e Leonnard sobre o caso

#  Finalização da discussão, Gaspard pode sair do apartamento nesse momento
func finished_discussion():
	Game.unlock_player_input()
	Game.scene_manager.gui.unlock_menu()
	Game.scene_manager.set_current_camera(get_node(\"cameras/rooms/main_room_view\"))
	can_exit_apartment = true

